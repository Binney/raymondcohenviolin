<h1>New music entry</h1>

<%= form_for @song do |f| %>
<%= render 'shared/error_messages', object: f.object %>
<div class="field">
	<%= f.label :title %>
	<%= f.text_field :title %>
	
	<%= f.label :url %>
	<%= f.text_field :url %>

	<%= f.label :description %>
	<%= f.text_area :description %>

	<%= f.label :album_id %>
	<%= f.text_field :album_id %>

</div>

<%= f.submit "Save" %>

<% end %>