<% provide :title, "Add picture to gallery" %>

<h1>Add picture to gallery</h1>

<%= form_for @picture do |f| %>

	<%= f.label :avatar %><br />
	<%= f.file_field :avatar %>

	<%= f.label :url, "Location" %>
	<%= f.text_field :url %>

	<%= f.label :description %>
	<%= f.text_area :description %>

	<%= f.submit "Create", class: "btn btn-large btn-primary" %>

<% end %>