<% provide :title, "Edit image" %>

<div class="container">

<%= form_for @picture do |f| %>

	<%= f.label :image %><br />
	<%= f.file_field :image %>

	<%= f.label :description %>
	<%= f.text_area :description %>

	<%= f.submit "Save changes", class: "btn btn-large btn-primary" %>

<% end %>

</div>